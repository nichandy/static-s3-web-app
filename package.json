{
  "name": "web-app-sandbox",
  "version": "1.0.0",
  "description": "Repository for expirementing with various web-app configurations, features and frameworks",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack serve --config/webpack.config.js --mode development",
    "start": "webpack-dev-server --mode development --config config/webpack.config.js --open --hot --history-api-fallback --env.PLATFORM=local --env.VERSION=stag",
