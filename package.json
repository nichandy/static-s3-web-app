    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
